<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <client>
      <endpoint address="net.pipe://localhost/MainApplication" binding="netNamedPipeBinding"
          bindingConfiguration="AppDirect.WindowsClient.MainApplication"
          contract="AppDirect.WindowsClient.Common.API.IMainApplication" name="AppDirect.WindowsClient.MainApplication">
      </endpoint>
    </client>
    <services>
      <service name="AppDirect.WindowsClient.Browser.API.BrowsersManagerApi" behaviorConfiguration="ServiceThrottlingBehavior">
        <endpoint address="" binding="netNamedPipeBinding" bindingConfiguration="AppDirect.WindowsClient.MainApplication"
                  name="AppDirect.WindowsClient.Browser.API.BrowsersManagerApi" contract="AppDirect.WindowsClient.Common.API.IBrowsersManagerApi">
        </endpoint>
        <endpoint address="mex" binding="mexNamedPipeBinding"
                  contract="IMetadataExchange">
        </endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="net.pipe://localhost/BrowsersManagerApi"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <bindings>
      <netNamedPipeBinding>
        <binding name="AppDirect.WindowsClient.MainApplication" transferMode="Buffered" transactionFlow="False" receiveTimeout="00:05:00"
                 closeTimeout="00:05:00" openTimeout="00:05:00" sendTimeout="00:05:00" />
      </netNamedPipeBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior name="Default">
          <serviceMetadata httpGetEnabled="false" />
        </behavior>
        <behavior name="ServiceThrottlingBehavior">
          <serviceMetadata httpGetEnabled="false" />
          <serviceThrottling
                          maxConcurrentCalls="2147483647"
                          maxConcurrentInstances="2147483647"
                          maxConcurrentSessions="2147483647"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v2.0.50727"/>
  </startup>
